{
  "version": 2,
  "builds": [
    {
      "use": "@vercel/node",
      "src": "api/index.ts"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.ts",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "PATCH",
        "OPTIONS",
        "DELETE",
        "HEAD"
      ]
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}