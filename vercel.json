{
  "version": 2,
  "builds": [
    {
      "use": "@vercel/node",
      "src": "src/main.ts"
    }
  ],
  "routes": [
    {
      "dest": "src/main.ts",
      "src": "/(.*)",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "PATCH",
        "OPTIONS",
        "DELETE",
        "HEAD"
      ]
    }
  ]
}